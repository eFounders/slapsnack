{
  "name": "slapsnack-frontend",
  "version": "0.1.0",
  "description": "SlapSnack",
  "author": "eFounders",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint .;exit 0",
    "prettier": "prettier --write \"{components,lib,pages}/**/*.js\"",
    "dev": "next",
    "build": "next build",
    "preexport": "npm run build",
    "export": "next export",
    "prestart": "npm run export",
    "start": "serve out",
    "predeploy": "npm run export && cp now.json out",
    "deploy": "cd out && now && now alias",
    "cleanup": "now rm slapsnack-frontend --safe -y"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "next": "^4.2.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "serve": "^6.4.8",
    "uploadcare-widget": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "prettier": "^1.10.2"
  }
}
